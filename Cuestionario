    private void ResultadoActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int contador=0, i=0;
        
        if(p1r1.isSelected()){
            resp[0]=1;
        }
        
        if(p2.getSelectedIndex()==4){
            resp[1]=1;
        }
        
        if(p3.getSelectedIndex()==3){
            resp[2]=1;
        }
        
        if("Roberto Lewandowski".equals(p4.getText())){
            resp[3]=1;
        }
        
        if(p6r3.isSelected()){
            resp[5]=1;
        }
        
        if("Lev Yashin".equals(p7.getText())){
            resp[6]=1;
        }
        
        if(p8r5.isSelected()){
            resp[7]=1;
        }
        
        if("Signal Iduna Park".equals(p10r1.getText())){
            resp[9]=1;
        }
        
        for(i=0; i<10; i++){
            contador=contador+resp[i];
        }
        
        JOptionPane.showMessageDialog(null, "Su calificacion es: "+contador);
        Resultado.setEnabled(false);
        
    }                                         

